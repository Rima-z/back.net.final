      "Order": {
        "required": [
          "customerName",
          "orderDate",
          "orderStatus"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "customerName": {
            "minLength": 1,
            "type": "string"
          },
          "orderDate": {
            "type": "string",
            "format": "date-time"
          },
          "totalPrice": {
            "type": "number",
            "format": "double"
          },
          "orderStatus": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "OrderCreateDto": {
        "type": "object",
        "properties": {
          "customerName": {
            "type": "string",
            "nullable": true
          },
          "orderDate": {
            "type": "string",
            "format": "date-time"
          },
          "orderStatus": {
            "type": "string",
            "nullable": true
          },
          "orderItems": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrderItemCreateDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "OrderItemCreateDto": {
        "type": "object",
        "properties": {
          "articleId": {
            "type": "integer",
            "format": "int32"
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          },
          "price": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "Payment": {
        "required": [
          "cardHolderName",
          "cardNumber",
          "cvv",
          "expirationDate",
          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "cardNumber": {
            "maxLength": 16,
            "minLength": 15,
            "type": "string",
            "format": "credit-card"
          },
          "cardHolderName": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "expirationDate": {
            "minLength": 1,
            "pattern": "^(0[1-9]|1[0-2])\\/([0-9]{2})$",
            "type": "string"
          },
          "cvv": {
            "minLength": 1,
            "pattern": "^\\d{3}$",
            "type": "string"
          },
          "amount": {
            "minimum": 0,01,
            "type": "number",
            "format": "double"
          },
          "isSuccessful": {
            "type": "boolean"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          }
        },
        "additionalProperties": false
      },
